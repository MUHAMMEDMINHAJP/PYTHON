   "source": [
    "#count the number of characters in a string\n",
    "s=input(\"enter a string:\")\n",
    "c={}\n",
    "for i in s:\n",
    "    if i in c:\n",
    "        c[i]=c[i]+1\n",
    "    else:\n",
    "        c[i]=1\n",
    "for key, value in c.items():\n",
    "    print(key,\"=>\",value);\n"
   ]
