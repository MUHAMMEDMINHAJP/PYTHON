    "# 43 check the given number is asmstrong or not\n",
    "n=int(input(\"enter the number: \"))\n",
    "sum=0\n",
    "temp=n\n",
    "while (temp>0):\n",
    "    s=temp%10\n",
    "    sum=sum+s**3\n",
    "    temp=temp//10\n",
    "if (n==sum):\n",
    "    print(n,\"is an armstrong number\")\n",
    "else:\n",
    "    print(n,\"is not armstrong number\")"
   ]
