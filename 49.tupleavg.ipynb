    "#49 average value of the numbers in a given tuple\n",
    "a=tuple(input (\"enter the numbers seperated by comma:\").split(','))\n",
    "l=len(a)\n",
    "c=0\n",
    "for i in range(l):\n",
    "    c=int(a[i])+c\n",
    "    avg=c/l\n",
    "print(\"the avg of the given number are:\",avg)"
